{
  "name": "rebass-json",
  "version": "2.0.0-0",
  "description": "React UI component library & design system, built with styled-components and styled-system",
  "scripts": {
    "start": "fuse-box-react-scripts start",
    "build": "fuse-box-react-scripts build",
    "dist": "node export && node build && tsc  -p ./tsconfig.dist.json",
    "test": "fuse-box-react-scripts test --env=jsdom",
    "eject": "fuse-box-react-scripts eject",
    "ejectconfig": "fuse-box-react-scripts ejectconfig",
    "tojson": "node index",
    "tojs": "node reverse"
  },
  "keywords": [
    "components",
    "react",
    "react-component",
    "ui",
    "design-system",
    "styled-system",
    "styled-components",
    "presentational",
    "stateless",
    "functional"
  ],
  "author": "OffGrid Networks",
  "license": "Apache 2.0",
  "dependencies": {
    "grid-styled": "3.2.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "styled-components": "3.2.0",
    "styled-system": "2.1.2",
    "system-components": "2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.40",
    "@babel/plugin-transform-react-jsx": "^7.0.0-beta.40",
    "@babel/standalone": "^7.0.0-beta.40",
    "chroma-js": "^1.3.6",
    "fs-extra": "^5.0.0",
    "fuse-box-react-scripts": "3.0.78",
    "fuse-box-react-scripts-ts": "3.0.78",
    "stringify-object": "^3.2.2",
    "typescript": "^2.7.2",
    "babel-runtime": "^6.26.0",
    "styled-system": "^2.2.1",
    "prop-types": "15.6.1",
    "axe-prop-types": "1.0.5"
  },
  "resolutions": {
    "styled-components": "3.2.0",
    "styled-system": "2.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jxnblk/rebass.git"
  },
  "directories": {
    "config": "config",
    "src": "src",
    "public": "docs/public"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  }
}
